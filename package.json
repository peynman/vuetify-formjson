{
    "name": "@peynman/vuetify-formjson",
    "description": "Generate Forms using JSON Schema. Based on Vuetify components.",
    "version": "0.0.10",
    "license": "GPL-3.0-or-later",
    "author": {
        "name": "Peynman",
        "email": "peynman.net@gmail.com",
        "url": "https://peynman.github.io/vuetify-formjson/"
    },
    "repository": "github:peynman/vuetify-formjson",
    "main": "dist/vuetify-formjson.umd.js",
    "module": "dist/vuetify-formjson.esm.js",
    "unpkg": "dist/vuetify-formjson.min.js",
    "keywords": [
        "vue",
        "vuetify",
        "form",
        "json",
        "schema",
        "input",
        "datatable"
    ],
    "browser": {
        "./components": "src/components.js"
    },
    "publishConfig": {
      "access": "public"
    },
    "scripts": {
        "build": "./node_modules/rollup/dist/bin/rollup --config rollup.config.js",
        "build:prod": "./node_modules/rollup/dist/bin/rollup --config rollup.config.js --config-production",
        "watch": "./node_modules/rollup/dist/bin/rollup --config rollup.config.js --config-iife --env debug -w",
        "watch:prod": "./node_modules/rollup/dist/bin/rollup --config rollup.config.js --config-iife --config-production -w",
        "dev": "./node_modules/npm-run-all/bin/npm-run-all/index.js --parallel start watch",
        "prod": "./node_modules/npm-run-all/bin/npm-run-all/index.js --parallel start watch:prod",
        "start": "./node_modules/serve/bin/serve.js dist",
        "lint": "./node_modules/eslint/bin/eslint.js src/**/**/**/*.{js,vue} src/**/**/*.{js,vue}  src/**/*.{js,vue} src/*.{js,vue} --fix",
        "test": "echo \"No test specified\""
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@rollup/plugin-buble": "^0.21.3",
        "@rollup/plugin-commonjs": "^11.1.0",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "@rollup/plugin-replace": "^2.3.2",
        "child_process": "^1.0.2",
        "eslint": "^6.8.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "eslint-plugin-vue": "^6.2.2",
        "npm-run-all": "^4.1.5",
        "rollup": "^1.27.14",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-eslint": "^7.0.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-postcss": "^2.9.0",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-terser": "^5.1.3",
        "rollup-plugin-uglify": "^6.0.4",
        "rollup-plugin-url-resolve": "^0.1.1",
        "rollup-plugin-vue": "^5.1.4",
        "rtlcss": "^2.4.1",
        "sass": "^1.26.5",
        "sass-loader": "^7.1.0",
        "serve": "^11.2.0",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "blockly": "^3.20200402.1",
        "jsoneditor": "^9.5.6",
        "markdown-it": "^10.0.0",
        "mathlive": "^0.50.7",
        "mermaid": "^8.5.0",
        "moment": "^2.26.0",
        "moment-timezone": "^0.5.31",
        "tree-model": "^1.0.7",
        "vue": "^2.6.11",
        "vue-axios": "^2.1.5",
        "vuetify": "^2.2.27",
        "vuetify-image-input": "^19.1.0",
        "vuetify-loader": "^1.4.3"
    }
}
